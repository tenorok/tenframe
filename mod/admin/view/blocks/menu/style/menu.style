.mod-admin-menu {
	float: left;
	margin: 50px 40px 0 20px;
	width: 150px;
	font-size: 14px;
	color: #333;
}
	.mod-admin-menu__item {
		margin-bottom: 5px;
	}
		.mod-admin-menu__item-wrap {
			padding: 3px 6px;
		}
		.mod-admin-submenu {
			margin: 5px 0 0 15px;
			font-size: 12px;
		}
			.mod-admin-submenu__item {
				padding: 3px 6px;
			}
	.mod-admin-menu__item_active {
		background-color: #F3EDC3;
	}
		.mod-admin-menu__url {
			color: #4F3FFF;
		}